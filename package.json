{
  "name": "Peeranha",
  "version": "0.0.1",
  "description": "Decentralized Q&A Platform",
  "repository": {
    "type": "git",
    "url": "git://github.com/peeranha/peeranha-web.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "webpack --config internals/webpack/webpack.prod.babel.js --color --progress",
    "build:test": "cross-env NODE_ENV=test npm run build",
    "build:staging": "cross-env NODE_ENV=staging npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=8192 node server ",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build:prod && npm run start:prod",
    "start:test": "cross-env NODE_ENV=test node server",
    "start:staging": "cross-env NODE_ENV=staging node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean:all": "npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage --runInBand --no-cache",
    "test:file": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": "> 1%, not dead",
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --write",
        "git add --force"
      ],
      "*.jsx": [
        "prettier --write",
        "git add --force"
      ],
      "*.ts": [
        "prettier --write",
        "git add --force"
      ],
      "*.tsx": [
        "prettier --write",
        "git add --force"
      ],
      "*.json": [
        "prettier --write",
        "git add --force"
      ]
    }
  },
  "pre-commit": "lint:staged",
  "dllPlugin": {
    "path": "public/vendors"
  },
  "jest": {
    "collectCoverageFrom": [
      "!app/*.{js}",
      "app/**/*.{js}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/app.js",
      "!app/global-styles.js",
      "!app/utils/web_integration/**/*.{js,jsx}",
      "!app/components/**/*.{js,jsx}",
      "!app/containers/App/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.svg.*$": "<rootDir>/internals/mocks/image.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/internals/testing/enzyme-setup.js",
      "<rootDir>/internals/testing/environment-variables.js"
    ],
    "testRegex": "tests/.*\\.test\\.js$",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
